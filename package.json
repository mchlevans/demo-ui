{
  "name": "autos-ui",
  "version": "1.0.0",
  "description": "Pages for the main site",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "npx webpack build --config webpack.config.js",
    "build-dev": "export mode=development && npm run build",
    "build-prod": "export mode=production && npm run build",
    "start": "export mode=development && npx webpack serve --hot --config webpack.config.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "npx eslint \"src/*.{ts,tsx}\"",
    "format": "npx prettier --write src/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mchlevans/autos-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/mchlevans/autos-ui#readme",
